{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "consistent-return": "off",
    "no-return-assign": "off",
    "func-names": "off",
    "no-use-before-define": "off",
    "no-underscore-dangle": "off"
  }
}